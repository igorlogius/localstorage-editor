{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "description": "Add, edit, copy, remove, export and import local- and session- storage data",
  "manifest_version": 2,
  "name": "LocalStorage Editor",
  "permissions": ["clipboardRead", "clipboardWrite", "<all_urls>"],
  "version": "1.3.18"
}
