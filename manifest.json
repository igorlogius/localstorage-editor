{
    "author": "igorlogius",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": { 
        "default_popup": "popup.html" 
    },
    "content_scripts": [
        {
          "js": [
            "content.js"
          ],
          "matches": [
            "<all_urls>"
          ]
        }
    ],
    "icons": { 
        "128": "icon.png"
    },
    "manifest_version": 2,
    "name": "LocalStorage Editor",
    "permissions": [
        "<all_urls>", 
        "clipboardWrite"
    ],
    "version": "1.2.7"
}
